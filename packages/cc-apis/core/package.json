{
    "name": "@adobe/firefly-services-sdk-core",
    "version": "0.1.0",
    "author": "Adobe",
    "license": "MIT",
    "description": "Adobe Firefly Services core library.",
    "keywords": [
        "adobe",
        "firefly services",
        "services",
        "sdk",
        "core"
    ],
    "repository": {
        "url": "https://github.com/Firefly-Services/firefly-services-sdk-js"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
